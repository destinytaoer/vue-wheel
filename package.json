{
  "name": "vue-wheel-1",
  "version": "0.0.2",
  "description": "使用 VUE 实现的 UI 组件库",
  "main": "dist/index.js",
  "scripts": {
    "start": "parcel index.html --no-cache",
    "build": "parcel index.js --no-cache --no-minify",
    "test:dev": "parcel watch test/* --no-cache & karma start",
    "test": "parcel build test/* --no-cache --no-minify && karma start --single-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/destinytaoer/vue-wheel.git"
  },
  "keywords": [
    "vue",
    "ui"
  ],
  "author": "destiny",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/destinytaoer/vue-wheel/issues"
  },
  "homepage": "https://github.com/destinytaoer/vue-wheel#readme",
  "dependencies": {
    "vue": "^2.6.10",
    "vue-hot-reload-api": "^2.3.3"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^3.0.0",
    "chai": "^4.2.0",
    "chai-spies": "^1.0.0",
    "karma": "^4.1.0",
    "karma-chai": "^0.1.0",
    "karma-chai-spies": "^0.1.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-sinon-chai": "^2.0.2",
    "mocha": "^6.1.4",
    "parcel-bundler": "^1.12.3",
    "sass": "^1.21.0",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "vue-template-compiler": "^2.6.10"
  },
  "alias": {
    "vue": "./node_modules/vue/dist/vue.common.js"
